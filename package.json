{
  "name": "nextjs-login",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma-dev-reset": "dotenv -e .env.local prisma migrate reset --",
